goal setEV_DMC021_50_PA1  {EV_DMC021_50_PA1 = D_DMC021_50_PA1 *EVF_DMC021_50_PA1}
goal setEV_RFS71A_50_PA1  {EV_RFS71A_50_PA1 = D_RFS71A_50_PA1 *EVF_RFS71A_50_PA1}
goal setEV_50_PA1         {EV_50_PA1        = EV_SJR062_50_PA1 + EV_DMC021_50_PA1 + EV_RFS71A_50_PA1}

goal setDP_SJR062_50_PA1  {DP_SJR062_50_PA1 = D_SJR062_50_PA1 *DPF_SJR062_50_PA1}
goal setDP_DMC021_50_PA1  {DP_DMC021_50_PA1 = D_DMC021_50_PA1 *DPF_DMC021_50_PA1}
goal setDP_RFS71A_50_PA1  {DP_RFS71A_50_PA1 = D_RFS71A_50_PA1 *DPF_RFS71A_50_PA1}
goal setDP_50_PA1         {DP_50_PA1        = DP_SJR062_50_PA1 + DP_DMC021_50_PA1 + DP_RFS71A_50_PA1}

goal setLF_SJR062_50_PA1  {LF_SJR062_50_PA1 = D_SJR062_50_PA1 *LFF_SJR062_50_PA1}
goal setLF_DMC021_50_PA1  {LF_DMC021_50_PA1 = D_DMC021_50_PA1 *LFF_DMC021_50_PA1}
goal setLF_RFS71A_50_PA1  {LF_RFS71A_50_PA1 = D_RFS71A_50_PA1 *LFF_RFS71A_50_PA1}
goal setLF_50_PA1         {LF_50_PA1        = LF_SJR062_50_PA1 + LF_DMC021_50_PA1 + LF_RFS71A_50_PA1}

goal setOS_SJR062_50_PA1  {OS_SJR062_50_PA1 = D_SJR062_50_PA1 *OSF_SJR062_50_PA1}
goal setOS_DMC021_50_PA1  {OS_DMC021_50_PA1 = D_DMC021_50_PA1 *OSF_DMC021_50_PA1}
goal setOS_RFS71A_50_PA1  {OS_RFS71A_50_PA1 = D_RFS71A_50_PA1 *OSF_RFS71A_50_PA1}
goal setOS_50_PA1         {OS_50_PA1        = OS_SJR062_50_PA1 + OS_DMC021_50_PA1 + OS_RFS71A_50_PA1}

goal setDL_SJR062_50_PA1  {DL_SJR062_50_PA1 = EV_SJR062_50_PA1  + DP_SJR062_50_PA1  + LF_SJR062_50_PA1  + OS_SJR062_50_PA1 }
goal setDL_DMC021_50_PA1  {DL_DMC021_50_PA1 = EV_DMC021_50_PA1  + DP_DMC021_50_PA1  + LF_DMC021_50_PA1  + OS_DMC021_50_PA1 }
goal setDL_RFS71A_50_PA1  {DL_RFS71A_50_PA1 = EV_RFS71A_50_PA1  + DP_RFS71A_50_PA1  + LF_RFS71A_50_PA1  + OS_RFS71A_50_PA1 }
goal setDL_50_PA1         {DL_50_PA1        = EV_50_PA1 + DP_50_PA1  + LF_50_PA1  + OS_50_PA1}

! reuse < sum of tailwater and canal seepage to drains
! reuse < (rufr * awr) + (rufo * awo)
goal set1RU_50_PA1        {RU_50_PA1  = min(TW_50_PA1,RUFR_50_PA1*AWR_50_PA1  + RUFO_50_PA1*AWO_50_PA1)} 
!zrc@ goal set2RU_50_PA1        {RU_50_PA1  < RUFR_50_PA1*AWR_50_PA1  + RUFO_50_PA1*AWO_50_PA1 } 
! applied water  = awr + awo
goal setAW_50_PA1         {AW_50_PA1  = AWR_50_PA1 + AWO_50_PA1 } 
! riparian losse = factor * aw
goal setRP_50_PA1         {RP_50_PA1  = AW_50_PA1  * RPF_50_PA1 } 



