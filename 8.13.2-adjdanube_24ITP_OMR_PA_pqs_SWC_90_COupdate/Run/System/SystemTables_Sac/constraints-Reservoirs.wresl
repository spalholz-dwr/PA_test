!@jmg 2017v13 - next 5 lines added from 5/31/2017 edits from Richard Chen (DWR)
define ETPenalty {value 900000}
!goal Evap_TRNTY {lhs E_TRNTY rhs (ER_TRNTY/24/cfs_af)*A_TRNTYlast+(ER_TRNTY/24/cfs_af)*A_TRNTY lhs<rhs penalty ETPenalty lhs>rhs constrain } 
goal   setE_LWSTN {lhs E_LWSTN rhs (ER_LWSTN/12/cfs_af)*A_LWSTN lhs<rhs penalty ETPenalty lhs>rhs constrain } 
goal   Evap_THRMF {lhs E_THRMF rhs (ER_THRMF/12/cfs_af)*A_THRMF lhs<rhs penalty ETPenalty lhs>rhs constrain }
!goal   evap_ENGLB {lhs E_ENGLB rhs (ER_ENGLB/12/cfs_af)*A_ENGLB lhs<rhs penalty ETPenalty lhs>rhs constrain }  !Z.Q. Richard Chen 5/31/2017

