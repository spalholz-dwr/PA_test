!define(fm) I33A_fm {case a{condition mv($m)>=JAN .and. mv($m)<=MAY select per90 from CAM_SJRES where month=mv($m),LOC=2}case b{condition always select per90 from CAM_SJRES where month=mv($m),LOC=2}}

!Average flows in taf on East Side streams (Mokelumne (includes Cosumnes), Calaveras, French Camp Slough)	
!Averages from a prior CalSim 3 run, averaged by Sac WY type	
define(fm) I33A_fm {
	case W {
	condition wyt_SAC == 1
	select W from CAM_ES_MONTHWYTYPE where month=mv($m) }
	case AN {
	condition wyt_SAC == 2
	select AN from CAM_ES_MONTHWYTYPE where month=mv($m) }
	case BN {
	condition wyt_SAC == 3
	select BN from CAM_ES_MONTHWYTYPE where month=mv($m) }
	case D {
	condition wyt_SAC == 4
	select D from CAM_ES_MONTHWYTYPE where month=mv($m) }
	case C {
	condition wyt_SAC == 5
	select C from CAM_ES_MONTHWYTYPE where month=mv($m) }
}

define(fm) I33A_dv_fm {alias I33A_fm($m) kind 'FLOW' units 'TAF'}                                                                                                                                                                                                                                                

