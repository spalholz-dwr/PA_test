 goal minPGERel1 {D_NBLDB_CGT005 = D_NBLDB_CGT005_POW_MIN + D_NBLDB_CGT005_POW_ADD}
 goal minPGERel2 {D_NBLDB_CGT005_POW_MIN < (min(max(0.,S_NBLDB(-1) - S_NBLDBlevel1 - 1.2 + (I_NBLDB + I_NFY029 - NBBFishFlow)*cfs_taf),18.500/1.13))*taf_cfs} ! evap. estimate is 1.2 taf
 