define addGP_Coeff	{case mOct {condition month == OCT .and. wytype_YUBA == 6 value 0.} 
			case mNov {condition month == NOV .and. wytype_YUBA == 6 value 0.} 
			case mDec {condition month == DEC .and. wytype_YUBA == 6 value 0.} 
			case mJan {condition month == JAN .and. wytype_YUBA == 6 value 0.} 
			case mFeb {condition month == FEB .and. wytype_YUBA == 6 value 0.} 
			case mMar {condition month == MAR .and. wytype_YUBA == 6 value 0.} 
			case mApr {condition month == APR .and. wytype_YUBA == 6 value 0.} 
			case mMay {condition month == MAY .and. wytype_YUBA == 6 value 0.20} 
			case mJun {condition month == JUN .and. wytype_YUBA == 6 value 0.20} 
			case mJul {condition month == JUL .and. wytype_YUBA == 6 value 0.25} 
			case mAug {condition month == AUG .and. wytype_YUBA == 6 value 0.25}
			case mSep {condition month == SEP .and. wytype_YUBA == 6 value 0.10} 
			case otherwise {condition always value 0.}}
			
