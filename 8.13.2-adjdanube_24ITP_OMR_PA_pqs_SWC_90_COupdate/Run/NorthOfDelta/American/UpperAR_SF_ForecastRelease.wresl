!SMUD operates for a combined UARP reservoir storage of 210 TAF by the end of December
!Compute the expected storage release based on the UpperAmerican cycle output
define UARP_Res_Release_Fcst {
	case JunSep {condition month <=3 .or. month >=9	value S_LOONL[UpperAmerican] + S_UNVLY[UpperAmerican] + S_ICEHS[UpperAmerican] - 100.} !Use 100 to better match historical operations
	case Other 	{condition always 					value 0.}
}
Define UARP_Res_Release_Fcst_DV {alias UARP_Res_Release_Fcst kind 'debug' units 'taf'}

!Accumulated accretions in case of storage release conditions
Define UARP_Accretion_Jun_Fcst {
	case PreJun {condition month  <9	value 0. }
	case Jun 	{condition month ==9	value I_SLF009_fcst(Jun-month) + I_SFA076_fcst(Jun-month) + I_SLV015_fcst(Jun-month) + I_SLV006_fcst(Jun-month) + I_BSH003_fcst(Jun-month) + I_SFA040_fcst(Jun-month) + I_RCK001_fcst(Jun-month) + I_SFA030_fcst(Jun-month) + I_WBR001_fcst(Jun-month) }
	case Other 	{condition always 		value 0.}	
}
Define UARP_Accretion_Jul_Fcst {
	case PreJun {condition month  <9	value 0. }
	case Jul 	{condition month <=10	value I_SLF009_fcst(Jul-month) + I_SFA076_fcst(Jul-month) + I_SLV015_fcst(Jul-month) + I_SLV006_fcst(Jul-month) + I_BSH003_fcst(Jul-month) + I_SFA040_fcst(Jul-month) + I_RCK001_fcst(Jul-month) + I_SFA030_fcst(Jul-month) + I_WBR001_fcst(Jul-month) }
	case Other 	{condition always 		value 0.}	
}
Define UARP_Accretion_Aug_Fcst {
	case PreJun {condition month  <9	value 0. }
	case Aug 	{condition month <=11	value I_SLF009_fcst(Aug-month) + I_SFA076_fcst(Aug-month) + I_SLV015_fcst(Aug-month) + I_SLV006_fcst(Aug-month) + I_BSH003_fcst(Aug-month) + I_SFA040_fcst(Aug-month) + I_RCK001_fcst(Aug-month) + I_SFA030_fcst(Aug-month) + I_WBR001_fcst(Aug-month) }
	case Other 	{condition always 		value 0.}	
}
Define UARP_Accretion_Sep_Fcst {
	case PreJun {condition month  <9	value 0. }
	case Sep 	{condition month <=12	value I_SLF009_fcst(Sep-month) + I_SFA076_fcst(Sep-month) + I_SLV015_fcst(Sep-month) + I_SLV006_fcst(Sep-month) + I_BSH003_fcst(Sep-month) + I_SFA040_fcst(Sep-month) + I_RCK001_fcst(Sep-month) + I_SFA030_fcst(Sep-month) + I_WBR001_fcst(Sep-month) }
	case Other 	{condition always 		value 0.}	
}

!Divide storage release between remaining months through the end of the year
Define UARP_Stor_SFA011_Jul_Fcst {
	case PreJun {condition month  <9	value 0. }
	case Jun	{condition month ==9	value (UARP_Res_Release_Fcst/((Sep-month) +3))*taf_cfs(Jul-month) + UARP_Accretion_Jul_Fcst}
	case other	{condition always	    value 0.}
}
Define UARP_Stor_SFA011_Aug_Fcst {
	case PreJun {condition month  <9	value 0. }
	case JunJul	{condition month <=10	value (UARP_Res_Release_Fcst/((Sep-month)+3))*taf_cfs(Aug-month) + UARP_Accretion_Aug_Fcst}
	case other	{condition always	    value 0.}
}
Define UARP_Stor_SFA011_Sep_Fcst {
	case PreJun {condition month  <9	value 0. }
	case JunAug	{condition month <=11	value (UARP_Res_Release_Fcst/((Sep-month)+3))*taf_cfs(Sep-month) + UARP_Accretion_Sep_Fcst}
	case other	{condition always	    value 0.}
}

!The calculation is intended to determine the remaining inflow AFTER the current month through either May or September
Define SFA_Release_Fcst {
	case PreFeb {condition month <5		value 0.}
	case Feb	{condition month ==5	value MIF_SFA011_Mar_Fcst*cfs_TAF(1) + MIF_SFA011_Apr_Fcst*cfs_TAF(2) + MIF_SFA011_May_Fcst*cfs_TAF(3)}
	case Mar	{condition month ==6	value MIF_SFA011_Apr_Fcst*cfs_TAF(1) + MIF_SFA011_May_Fcst*cfs_TAF(2)}
	case Apr	{condition month ==7	value MIF_SFA011_May_Fcst*cfs_TAF(1)}
	case May	{condition month ==8	value 0.}
	case Jun	{condition month ==9	value max(UARP_Stor_SFA011_Jul_Fcst*cfs_taf(1) + UARP_Stor_SFA011_Aug_Fcst*cfs_taf(2) + UARP_Stor_SFA011_Sep_Fcst*cfs_taf(3), MIF_SFA011_Jul_Fcst*cfs_taf(1) + MIF_SFA011_Aug_Fcst*cfs_taf(2) + MIF_SFA011_Sep_Fcst*cfs_taf(3))}
	case Jul	{condition month ==10	value max(UARP_Stor_SFA011_Aug_Fcst*cfs_taf(1) + UARP_Stor_SFA011_Sep_Fcst*cfs_taf(2), MIF_SFA011_Aug_Fcst*cfs_taf(1) + MIF_SFA011_Sep_Fcst*cfs_taf(2))}
	case Aug	{condition month ==11	value max(UARP_Stor_SFA011_Sep_Fcst*cfs_taf(1), MIF_SFA011_Sep_Fcst*cfs_taf(1))}
	case Sep	{condition month ==12	value 0.}
}
Define SFA_Release_Fcst_DV {alias SFA_Release_Fcst kind 'Forecast' units 'TAF'}
