define MIF_UARP_SSV013_Feb_Fcst{
	case PreFeb	{condition month < 5 value 0.}
	case Wet 	{condition month == 5. .and. UARPYrTypedv[UpperAmerican] == 1 select Wet         	from UARPmif_SSV013 where month = 5 }
	case AN  	{condition month == 5. .and. UARPYrTypedv[UpperAmerican] == 2 select AboveNormal 	from UARPmif_SSV013 where month = 5 }
	case BN  	{condition month == 5. .and. UARPYrTypedv[UpperAmerican] == 3 select BelowNormal 	from UARPmif_SSV013 where month = 5 }
	case Dry 	{condition month == 5. .and. UARPYrTypedv[UpperAmerican] == 4 select Dry     		from UARPmif_SSV013 where month = 5 }
	case Crit	{condition month == 5. .and. UARPYrTypedv[UpperAmerican] == 5 select Critical	 	from UARPmif_SSV013 where month = 5 }
	case Other 	{condition always value 0.}
}
