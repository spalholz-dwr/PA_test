! Distribute San Luis SWP carryover deliveries throughout year for select contractors based on drought years carryover behavior.
define wyt_SAC_A56 {
    case OCTtoDec {
             condition  month >= OCT .and. month <= DEC 
             value wyt_Sac_(PrevMay)
             }
        case JuntoSep {
             condition  month >= Jun .and. month <= Sep 
             value wyt_Sac_(PrevMay)
             }
        case rest {
             condition  always
             value WYT_SAC_Frcst}
}
define wyt_SAC_A56_{alias wyt_SAC_A56 kind 'wateryeartype' units 'none'}

define co_fraction_to_Sep {
	case EarlyFullSanLuis {
		condition	wyt_SAC_A56 < 3.5
		value		co_fraction
	}
	case OcttoDec {
		condition	range(month,oct,dec)
		value		1.
	}
	case otherwise {
		condition	always
		value		1./(13. - real(month))
	}
}
