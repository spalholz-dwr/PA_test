! lost carryover storage due to full San Luis (lost equal to unused banks capacity) and Article 21 deliveries made
define co_spill {
  case a  {condition month==oct .and. wateryear==bgnWY value 0.}
  case b  {condition month==jan             value 0.}
  case c  {condition S_SLUIS_SWP(-1)>=1067                    value min(co_req_tot_sod, max(0.,swp_IN_total(-1)-swp_IN_Feath(-1)-swp_IN_NBay(-1))*cfs_taf(-1)
  																+ min(max(0.,6680.-C_CAA003(-1)) * cfs_taf(-1), (NDOI_ADD_CVP(-1) + NDOI_ADD_SWP(-1))*cfs_taf(-1))) }
  case d  {condition always                           value min(co_req_tot_sod, max(0.,swp_IN_total(-1)-swp_IN_Feath(-1)-swp_IN_NBay(-1))*cfs_taf(-1))}
}
define co_spill_dv {alias co_spill kind 'carryover' units 'taf'}

! Another loss of carryover can be insufficient storage in San Luis at the beginning of January.
define co_unavail {
    case January {
        condition month == jan
        value max(0., co_req_tot_sod - (S_SLUIS_SWP(-1) - 42.))
    }
    case otherwise {
        condition always
        value 0.
    }
}
define co_unavail_dv {alias co_unavail kind 'carryover' units 'taf'}

! Total carryover shortage will be the maximum of the spill or unavailable.  When one is greater than zero,
!it is likely that the other will be zero.
define co_lost {value max(co_spill, co_unavail)}
define co_lost_dv {alias co_lost kind 'carryover' units 'taf'}
define tot_CO_account_dv {std kind 'CARRYOVER' units 'TAF'} !define del_swp_codv {std kind 'DEL-TARGET' units 'TAF'} !


! PQS:  ! PSpalholz 20250115
define tot_CO_account {
	case jan       {condition month == jan value max(0.,tot_CO_account_dv(-1) - co_lost)                          	 }
	case otherwise {condition always       value max(0.,tot_CO_account_dv(-1) - co_lost - SWP_CO_TOTAL(-1)*cfs_taf) }
}
goal set_tot_CO_account {tot_CO_account_dv = tot_CO_account}

!Determine carryover available after losses subtracted.
define co_available {value max(0., co_req_tot - co_lost)}
goal set_co_available {co_available_dv = co_available}  !dvar defined in wsi_swp.wresl

define co_available_sod {value max(0., co_req_tot_sod - co_lost)}
goal set_co_available_sod {co_available_sod_dv = co_available_sod}  !dvar defined in wsi_swp.wresl

